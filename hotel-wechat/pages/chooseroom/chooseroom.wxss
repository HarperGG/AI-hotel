.content-item2{
  position: relative;
  display: flex;
  align-items: center;
  flex-wrap: wrap;
  width: 44%;
  height: 350rpx;
  background:rgba(248, 250, 249, 1);
  border:2rpx solid rgba(64, 64, 64, 1);
  border-radius:10rpx;
}
.content-item2 image{
  width: 100%;
  height: 200rpx;
}
.content-item2 text{
  margin-left: 20rpx;
}
.content-item2 text:nth-of-type(1) {
  width: 100%;
  font-size:20rpx;

  font-weight:400;
  color:rgba(13, 59, 46, 1);
}
.content-item2 text:nth-of-type(2) {
  
  font-size: 30rpx;
  font-weight: 600;
  color: rgba(13, 59, 46, 1);
}
.content-item2 text:nth-of-type(3) {

  font-size: 20rpx;
  font-weight: 400;
  color: rgba(13, 59, 46, 1);
}
.content-item2 text:nth-of-type(4) {
  width: 100%;
  font-size:40rpx;
  font-weight:600;
  color:rgba(242, 182, 8, 1);
}
.content-item2 text:nth-of-type(5) {
  width: 100%;
  position: absolute;
  top: 0;
  left: -6%;
  height: 200rpx;
  z-index: 20;
  border-radius:10rpx;
  background: rgba(0, 0, 0, .2);
}
/* 全局的蒙版 */
.masking{
  height: 1060rpx;
  width: 100%;
  position: absolute;
  top: 150rpx;
  left: 0;
  background: rgba(0, 0, 0, .3);
  z-index: 1000;
}