.search{
  width: 100%;
  height: auto;
  display: flex;
  position: relative;
  align-items: center;
  justify-content: space-around;
}
.search-time{
  width: 110rpx;
  height: 50rpx;
  display: flex;
  flex-wrap: wrap;
  display: inline-block;
  background:rgba(13, 59, 46, 1);
  border-radius:10rpx;
  font-size:12rpx;
  font-family:FZHei-B01S;
  letter-spacing: 5rpx;
  box-sizing: border-box;
  padding: 5rpx 0 0 0;
  text-align: center;
  line-height: 20rpx;
  
  font-weight:400;
  color:rgba(252, 251, 249, 1);
}
.search-time text{
  width: 100rpx;
  margin-top: 20rpx;
}
.weui-input{
  font-size: 20rpx;
  box-sizing: border-box;
  padding: 0 60rpx 0 20rpx;
  width:510rpx;
  height:60rpx;
  border:5rpx solid rgba(13, 59, 46, 1);
  border-radius:8rpx;
}
.search-icon{
  position: absolute;
  right: 50rpx;
}

.filtrate{
  position: relative;
  margin-top: 20rpx;
display: flex;
justify-content: space-between;
box-sizing: border-box;
padding: 0 40rpx;
width: 100%;
height: auto;
}
.filtrate-item{
  width: 20%;
  height: auto;
  font-size:38rpx;
  font-weight:400;
  color:rgba(2, 2, 2, 1);
}
/* 下拉菜单 */
.priceDropBox{
  z-index: 1001;
  position: absolute;
  top: 50rpx;
  left: 0;
  width: 100%;
  background: #fff;
  border-top: 2rpx solid rgba(0, 0, 0, .1);
  height: auto;
  box-sizing: border-box;
  padding: 0 40rpx 20rpx 40rpx;
}
.priceDropBox-title{
  font-size: 30rpx;
  color: rgba(0, 0, 0, .8);
  font-weight: 500;
  margin-bottom: 20rpx;
}
.priceDropBox-info{
  width: 100%;
  height: auto;
  display: flex;
  justify-content: space-between;
  align-content: flex-start;
  flex-wrap: wrap;
}
.priceDropBox-info text{
  width: 23%;
  height: 60rpx;
  margin-top: 20rpx;
  background: rgba(0, 0, 0, .05);
  border-radius: 5rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 20rpx;
  color: rgba(0, 0, 0, .6);
}
/* 排序下拉框样式 */
.sortDropBox{
   z-index: 1001;
   position: absolute;
   top: 50rpx;
   left: 0;
   width: 100%;
   background: #fff;
   border-top: 2rpx solid rgba(0, 0, 0, .1);
   height: auto;
   box-sizing: border-box;
   display: flex;
   flex-wrap: wrap;
   padding: 0 40rpx 20rpx 40rpx;
}
.sortDropBox text{
  border-bottom: 2rpx solid rgba(0, 0, 0, 0.2);
  width: 100%;
  height: 80rpx;
  padding-left: 10rpx;
  line-height: 30rpx;
  font-size: 30rpx;
  color: rgba(0, 0, 0, .6);
  text-align: left;
}
/* 筛选下拉框 */
.filterDropBox{
  z-index: 1001;
  position: absolute;
  top: 50rpx;
  left: 0;
  width: 100%;
  background: #fff;
  border-top: 2rpx solid rgba(0, 0, 0, .1);
  height: auto;
  
}
.filterItem{
  width: 100%;
  box-sizing: border-box;
  padding: 0 40rpx 20rpx 40rpx;
  height: auto;
  border-bottom: 1rpx solid rgba(0, 0, 0, .3);
  padding-bottom: 40rpx;
  margin-top: 40rpx;
}
.filterItem-title {
font-size: 30rpx;
color: rgba(0, 0, 0, .8);
font-weight: 500;
margin-bottom: 20rpx;
margin-top: 40rpx;

}
.filterItem-info{
  
 width: 100%;
 height: auto;
 display: flex;
 justify-content: space-between;
 flex-wrap: wrap;
 align-content: flex-start;
}

.filterItem-info text {
  flex: 0 0 23%;
  width: 23%;
  line-height: 22rpx;
  height: 60rpx;
  margin-top: 25rpx;
  background: rgba(0, 0, 0, .05);
  border-radius: 5rpx;
  display: flex;
  justify-content: center;
  font-size: 20rpx;
  color: rgba(0, 0, 0, .6);
}
/* 筛选下拉框，按钮 */
.filterDropBox-btn{
  position: absolute;
  top: 600rpx;
  height: 80rpx;
  width: 100%;
  display: flex;
  box-shadow: 0 0 5rpx #ccc;
}
.filterDropBox-btn view:nth-of-type(1){
  height: 100%;
  width: 50%;
  display: flex;
  justify-content: center;
  align-items: center;
  background: #fff;
  font-size: 35rpx;
  color: rgba(0, 0, 0, .8);
}
.filterDropBox-btn view:nth-of-type(2) {
  height: 100%;
  width: 50%;
  display: flex;
  justify-content: center;
  align-items: center;
  background: #fff;
  font-size: 35rpx;
    background:rgba(13, 59, 46, 1);
    color: #fff;
   
}

/* 选择价格样式 */
.priceDropBox-info-textColor{
  background:rgba(13, 59, 46, 1) !important;
  color:rgba(242, 182, 8, 1) !important;
}
/* 选择排序样式 */
.sort-textstyle{
  color:rgba(242, 182, 8, 1) !important;
}

.vip{
  width: 100%;
  height: 100rpx;
  letter-spacing: 10rpx;
  background:rgba(109, 151, 115, 1);
  margin-top: 20rpx;
  display: flex;
box-sizing: border-box;
padding: 0 20rpx;
justify-content: space-between;
font-size:30rpx;
font-weight:400;
color:rgba(242, 182, 8, 1);
align-items: center;
}
.vip-title{
  width:300rpx;
  height:50rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  background:rgba(13, 59, 46, 1);
  border-radius:8rpx;
}
.content{
  box-sizing: border-box;
  padding: 0 40rpx;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  margin-top: 20rpx;

  width: 100%;
  height: auto;
}
.container::after {
  content: '';
  flex: auto;
  /* 或者flex: 1 */
}
